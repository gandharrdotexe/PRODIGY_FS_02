<%- include('layouts/header') %>

    <div class="container">
        <div class="row my-5">
            <div class="col-lg-12">
                <% if(message){ %>
                    <div class="alert alert-dismissable fade chow alert-<%= message.type %>" role="alert">
                        <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
                        <strong><%= message.message %></strong>
                    </div>
                <% } %>
                <div class="table-responsive">
                    <table class="table table-striped text-center">
                        <thead>
                            <tr class="table-dark">
                                <th>ID</th>
                                <th>Images</th>
                                <th>Name</th>
                                <th>Email</th>
                                <th>Phone</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>

<%- include('layouts/footer') %>